<%= form_for @artist do |a| %>

  <div class="form-group">
  <%= a.label :name %>
  <%= a.text_field :name %>
  </div>

  <div class="form-group">
  <%= a.label :hometown %>
  <%= a.text_field :hometown %>
  </div>

  <div class="form-group">
  <%= a.label :label %>
  <%= a.text_field :label %>
  </div>

  <div class="form-group">
  <%= a.label :bio %>
  <%= a.text_field :bio %>
  </div>

  <div class="form-group">
  <%= a.submit %>
  </div>

<% end %>