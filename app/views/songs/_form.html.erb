<%= form_for @song do |s|%>
  <p>
  <%= s.label :name %>
  <%= s.text_field :name %>
  </p>
  <p>
  <%= s.label :year %>
  <%= s.text_field :year %>
  </p>
  <p>
  <%= s.label :genre %>
  <%= s.text_field :genre %>
  </p>
  <p>
  <%= s.label :duration %>
  <%= s.text_field :duration %>
  </p>
  <p>
  <%= s.label :artist_id %>
  <%= s.collection_select :artist_id, Artist.all, :id, :name %></p>
  <p>
  <%= s.submit %>
  </p>
<% end %>